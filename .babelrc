{
    "env": {
        "development": {
            "plugins": [
                "flow-react-proptypes"
            ]
        },
        "production": {
            "plugins": [
                "@babel/transform-react-constant-elements",
                "@babel/transform-react-inline-elements"
            ]
        }
    },
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry"
            }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
    ]
}
