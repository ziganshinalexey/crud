{
  "name": "frontend-starter-kit",
  "version": "0.1.0",
  "author": "imprevo",
  "license": "ISC",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report",
    "lint:js": "eslint src --ext .js,.jsx",
    "lint:css": "stylelint src/**/*.{css,less}"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@userstory/eslint-config": "git+ssh://git@gitlab.userstory.ru:userstory-dev/eslint-config.git",
    "@userstory/stylelint-config": "git+ssh://git@gitlab.userstory.ru:userstory-dev/stylelint-config.git",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-flow-react-proptypes": "^25.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.13.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.92.0",
    "flow-coverage-report": "^0.6.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-gradient-transparency-fix": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "speed-measure-webpack-plugin": "^1.3.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
